<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="mosq_reject_wizard_form" model="ir.ui.view">
        <field name="name">mosq.reject.wizard.form</field>
        <field name="model">mosq.reject.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                  <field name="reject_reason"/>
                </sheet>
                <footer>
                    <button name="action_ok" string="Ok" type="object" default_focus="1" class="oe_highlight"/>
                </footer>
            </form>
        </field>
    </record>



</odoo>