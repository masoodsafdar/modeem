            </field>

             <p>المعلمين</p>
             <field name="best_teachers" >
              <tree>
                <field name="name" />
                <field name="rate" />
              </tree>
            </field>
          </sheet>
            </form>
      </field>
    </record>

 <act_window 
      id="top_five_action"
      name="top_five"
      res_model="top.five"
      view_mode="tree,form"/>



