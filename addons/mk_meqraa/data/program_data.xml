		    <field name="program_id" ref="mk_meqraa.program_meqraa"/>
		    <field name="lessons_minimum_audit">5</field>
		    <field name="lessons_memorize">5</field>
		</record>
		<record id="approache_meqraa3" model="mk.approaches">
		    <field name="name">منهج حفظ في ثلاث سنوات</field>
		    <field name="is_approache_meqraa" eval="True"/>
