<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    
		<record id="program_meqraa" model="mk.programs">
		    <field name="name">برنامج المقراة(حفظ)</field>
		    <field name="state">active</field>
